{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": [2, {
          "code": 500,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignorePattern": "^goog\\.(module|require)"
        }]
    }
}
